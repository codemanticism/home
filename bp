	char compare(char* stringA, char* stringB){
	for(unsigned int i = 0;i < 32; i += 1){
		if(*stringA != *stringB){
			return 0;
		}
		stringA += 1;
		stringB += 1;
		if((*stringA == '\0') && (*stringB == '\0')){
			return 1;		
		}else if((*stringA == '\0') || (*stringB == '\0')){
			return 0;
		}
	}
	return 1;
}

struct array reading_file(){
	FILE* fptr;
	fptr = fopen("list.txt","r");
	struct array array_ = create(16, 0, 1);
	if(fptr == NULL){
		printf("The file does not exist.\n");
	}
	struct array array_str = create(2, 1, 1);
	((char*)array_str.ptr)[1] = '\0';
	do
	{
		char c = fgetc(fptr);
		if(feof(fptr))
			break;
		((char*)array_str.ptr)[0] = c;
		append(&array_, array_str, 1);					
	} while(1);
	return array_;
}

