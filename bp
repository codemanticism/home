	append(array_, struct array arr2, unsigned int size_of);
	if(fptr == NULL){
		printf("The file does not exist.\n");
	}
	struct array array_str = create(2, 1, 1);
	do
	{
		char c = fgetc(fp);
		array_str.ptr[1] = '\0';
		if(feof(fp))
			break;
		array_str.ptr[0] = c;
		append(&array_, array_str, 1);					
	} while(1);
	array_str.ptr[0] = '\0';
	append(&array_, array_str, 1);				
	return array_.ptr;
}
