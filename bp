char compare(char* stringA, char* stringB){
	for(unsigned int i = 0;i < 32; i += 1){
		if(*stringA != *stringB){
			return 0;
		}
		stringA += 1;
		stringB += 1;
		if((*stringA == '\0') && (*stringB == '\0')){
			return 1;		
		}else if((*stringA == '\0') || (*stringB == '\0')){
			return 0;
		}
	}
	return 1;
}

unsigned int reading_file(some_file){
	FILE* fptr;
	fptr = fopen("list.txt","r");
