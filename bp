unsigned int count_length(char* str){
	unsigned int i = 0;
	for(; str[i] != '\0'; i++){
	}
	return i;
}
bool is_number(char letter){
	if((letter >= '0') && (letter <= '9')){
		return true;
	}
	return false;
}
bool is_letter(char letter){
	if((letter >= 'a') && (letter <= 'z')){
		return true;
	}else if((letter >= 'A') && (letter <= 'Z')){
		return true;
	}
	return false;
}
array ast_generator(char* code){
	array ast = create(16,0,1);
	array item = create(1,1,1);
	for(unsigned int i = 0; code[i] != '\0';){
		if(is_letter(code[i])){
			item[0] = &code[i];
			append(&ast, item, 1);	
			while(is_letter(code[i])){
				i++;
			}
		}else if(is_number(code[i])){
			item[0] = &code[i];
			append(&ast, item, 1);	
			while(is_number(code[i])){
				i++;
			}
		}else{
			char* compare_against = "+-*/=!";
			for(unsigned int i = 0; i < (sizeof(compare_against) - 1); i++){
				item[0] = compare_against;
				append(&ast, item, 1);	
			}
			i++;
			if(code[i] == '='){
				i++;
			}
		}
	}
	free(ast.ptr);
	free(item.ptr);
	return ast;	
}
bool compare(char* stringA, char* stringB){
	for(unsigned int i = 0;i < 32; i += 1){
		if(*stringA != *stringB){
			return false;
		}
		stringA += 1;
		stringB += 1;
		if((is_letter(*stringA) == 0) && (is_letter(*stringB) == 0)){
			return true;		
		}else if((is_letter(*stringA) == 0) || (is_letter(*stringB) == 0)){
			return false;
		}
	}
	return true;
}

struct array reading_file(char* str){
	FILE* fptr;
	fptr = fopen(str,"r");
	struct array array_ = create(16, 0, 1);
	if(fptr == NULL){
		printf("The file does not exist.\n");
	}
	struct array array_str = create(1, 1, 1);
	((char*)array_str.ptr)[1] = '\0';
	do
	{
		char c = fgetc(fptr);
		if(feof(fptr))
			break;
		((char*)array_str.ptr)[0] = c;
		append(&array_, array_str, 1);					
	} while(1);
	free(array_.ptr);
	free(array_str.ptr);	
	return array_;
}
void writing_files(char* str, char* another_str){
	FILE *fptr = fopen(str, "w");
	fprintf(fptr, another_str);
	fclose(fptr);
}
